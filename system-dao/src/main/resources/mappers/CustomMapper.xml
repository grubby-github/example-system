<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gaoz.example.dao.mapper.CustomMapper">
    <insert id="insert" parameterType="java.lang.String">${value}</insert>
    <delete id="delete" parameterType="java.lang.String">${value}</delete>
    <update id="update" parameterType="java.lang.String">${value}</update>
    <select id="select" resultType="java.util.HashMap" useCache="false">${value}</select>
    <select id="selectCount" resultType="java.lang.Integer" useCache="false">${value}</select>
</mapper>